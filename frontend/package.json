    "jest-each": "^27.1.0",
    "prettier": "^2.2.1"
  },
  "resolutions": {
    "babel-loader": "8.1.0"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      }
    }
